<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN">
<html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <title>Bitbee Flash Player</title>
        <meta content="text/html; charset=UTF-8" http-equiv="Content-Type">
        <script type="text/javascript" src="/js/jquery/jquery-1.8.0.min.js"></script>
        <script type="text/javascript" src="/js/flash/swfobject.js"></script>
        <script type="text/javascript" src="/js/flash/play.js"></script>
        <style media="screen" type="text/css">body{padding:0px;margin:0px}</style>
    </head>
    <body>
        <object data="/js/flash/player.swf" name="myDynamicContent" id="myDynamicContent" type="application/x-shockwave-flash" width="550" height="430">
            <param value="false" name="menu">
            <param value="true" name="allowfullscreen">
            <param value="transparent" name="wmode">
            <param value="always" name="allowscriptaccess">
            <param value="file=/upload/video/ND2_400.flv&amp;autostart=true" name="flashvars">
        </object>
    </body>
</html>
